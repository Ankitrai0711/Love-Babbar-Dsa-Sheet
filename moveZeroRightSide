import java.util.Arrays;
class Main{
    public static void main(String [] args){
        int arr[] = {1,0,0,2,3,0};
        int n = arr.length;
        int nzi=0;
        for(int i =0;i<n;i++){
            if(arr[i]!=0){
                arr[nzi]=arr[i];
                nzi++;
            }
        }
        for(int i=nzi;i<n;i++){
            arr[i]=0;
        }
        System.out.print(Arrays.toString(arr));
    }
}
