import java.util.*;
class prime
{
    public static void main(String[] args)
    {
        int n ;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the Number: ");
         n = sc.nextInt();
        int count = 0;
        for(int i =1;i<=n;i++)
        {
            if(n%i==0)
            {
                count++;
            }
        }
        if(count == 2)
        {
            System.out.println("Its a Prime Number..");
        }
        else
        {
            System.out.println("Not Prime Number..");
        }
    }
}
